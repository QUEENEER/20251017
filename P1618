#include <bits/stdc++.h>
using namespace std;
#define int long long

int n,m,j;
int arr[10];
signed main()
{
	int a,b,c,p=0;
	cin>>a>>b>>c;
	for(int i=1;i<987;i++){
		if(a*i<=987){
			arr[a*i%10]++;
			arr[a*i/10%10]++;
			arr[a*i/100]++;
		}
		if(b*i<=987){
			arr[b*i%10]++;
			arr[b*i/10%10]++;
			arr[b*i/100]++;
		}
		if(c*i<=987){
			arr[c*i%10]++;
			arr[c*i/10%10]++;
			arr[c*i/100]++;
		}
		for(j=1;j<=9;j++){
			if(arr[j]==0){
				break;
			}
		}
		if(j>=10){
			cout<<a*i<<' '<<b*i<<' '<<c*i<<'\n';
			p=1;
		}
		for(j=1;j<=9;j++)
			arr[j]=0;
	}
	if(p==0){
		cout<<"No!!!";
	}
}
